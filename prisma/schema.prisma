// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./cv-data.sqlite"
}

model Jobs {
  id          Int @id @default(autoincrement())
  position    String
  enterprise  String
  fromDate    DateTime
  toDate      DateTime
  place       String
  goals       String
}